###  http://purl.org/nidash/nidm#Acquisition

nidm:Acquisition rdf:type owl:Class ;

               rdfs:label "Acquisition" ;

               obo:IAO_0000115 "An acquisition is an activty in which data about a subject is gathered."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                 
               rdfs:subClassOf prov:Activity .




###  http://purl.org/nidash/nidm#AcquisitionDeviceOperator

nidm:AcquisitionDeviceOperator rdf:type owl:Class ;
                                  
               rdfs:label "Acquisition Device Operator" ;

               obo:IAO_0000115 "An acquisition device operator is a role of an agent who/that controls the device that collects the acquisition object."^^xsd:string ;
                        
               obo:IAO_0000114 obo:IAO_0000428 ;

               rdfs:comment "The use of who/that indicates that the agent performing this role can either be a person or another device." ;
                                  
               rdfs:subClassOf prov:Role .




###  http://purl.org/nidash/nidm#AcquisitionMethod

nidm:AcquisitionMethod rdf:type owl:Class ;
                       
               rdfs:label "Acquisition Method" ;

               obo:IAO_0000115 "An acquisition method is a specific form of a procedure that was used to gather data from a subject."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                       
               rdfs:subClassOf nidm:AcquisitionObjectQuality .



###  http://purl.org/nidash/nidm#AcquisitionModality

nidm:AcquisitionModality rdf:type owl:Class ;
                         
               rdfs:label "Acquisition Modality" ;

               obo:IAO_0000115 "An acquisition modality is a description of the general type of experiment that was used to gather data from a subject."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                         
               rdfs:subClassOf nidm:AcquisitionObjectQuality .



###  http://purl.org/nidash/nidm#AcquisitionObject

nidm:AcquisitionObject rdf:type owl:Class ;
                       
               rdfs:label "Acquisition Object"^^xsd:string ;
                       
               rdfs:subClassOf prov:Entity ;
                       
               obo:IAO_0000115 "An acquisition object is an entity that is gathered from a subject during an acquisition activity."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                       
               obo:IAO_0000117 <http://purl.org/nicholsn/card.ttl> .



###  http://purl.org/nidash/nidm#AcquisitionObjectQuality

nidm:AcquisitionObjectQuality rdf:type owl:Class ;

               rdfs:label "Acquisition Object Quality"^^xsd:string ;

               obo:IAO_0000115 "An acquisition object quality is an quality that describes an essential feature of an object that is gathered from a subject during an acquisition activity."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                              
               rdfs:subClassOf obo:PATO_0000001 .



###  http://purl.org/nidash/nidm#Amperometry

nidm:Amperometry rdf:type owl:Class ;
                 
               rdfs:label "Amperometry" ;

               obo:IAO_0000115 "Amperometry is an extracellular acquisition method in electrophysiology in which an electrode is brought near a cell to detect the release of ions by vesicles."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                 
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#Anatomical

nidm:Anatomical rdf:type owl:Class ;

               rdfs:label "Anatomical" ;

               obo:IAO_0000115 "Anatomical is a description of the usage of a given acquisition object, indicating that the purpose of the object is to provide information on the anatomy of the object on which the acquisition was performed."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                
               rdfs:subClassOf nidm:ImageUsageType .



###  http://purl.org/nidash/nidm#ArterialSpinLabeling

nidm:ArterialSpinLabeling rdf:type owl:Class ;
                                   
               rdfs:label "Arterial Spin Labeling"^^xsd:string ;

               obo:IAO_0000115 "Arterial spin labeling is a magnetic resonance imaging acquisition method that measures perfusion using the flow of saturated blood into a predefined area by comparing the image intensity obtained in the slice with and without the saturation pulse."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;
                                   
               rdfs:subClassOf nidm:AcquisitionMethod .




###  http://purl.org/nidash/nidm#AuxiliaryFile

nidm:AuxiliaryFile rdf:type owl:Class ;
                   
               rdfs:label "Auxiliary File"^^xsd:string ;
                   
               rdfs:subClassOf sio:SIO_000396 ;
                   
               obo:IAO_0000115 "An auxiliary file is an information-bearing object that is either used to inform the execution of an experiment or that is generated during the execution of the experiment."^^xsd:string ;

               rdfs:comment "An example of a file that informs the execution of an experiment is a b-vector file in diffusion-weighted MR imaging.  An example of a file that is generated during the execution of the experiment is a stimulus response file in functional MR imaging)"^^xsd:string ;
                   
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#AuxiliaryFileCollection

nidm:AuxiliaryFileCollection rdf:type owl:Class ;
                             
               rdfs:label "Auxiliary File Collection"^^xsd:string ;
                             
               rdfs:subClassOf prov:Collection ;
                             
               obo:IAO_0000115 "An auxiliary file collection is a grouping that contains the URI's of files that are necessary for the execution of the experiment or the processing of the data."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;

               rdfs:comment "An example of an auxiliary file collection is the b-value and b-vector files in diffusion-weighted MR imaging."^^xsd:string ;
                             
               prov:wasAssociatedWith nidm:AcquisitionObject .



###  http://purl.org/nidash/nidm#BehavioralInstrument

nidm:BehavioralInstrument rdf:type owl:Class ;

               rdfs:label "Behavioral Instrument"^^xsd:string ;

               obo:IAO_0000115 "A behavioral instrument is a instrument usage type that signifies that the type of data to be collected by the instrument is behaviorally-related data."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;
               
               rdfs:subClassOf nidm:InstrumentUsageType .




###  http://purl.org/nidash/nidm#NIDM_0000152

nidm:NIDM_0000152 rdf:type owl:Class ;
                  
               rdfs:label "Block-Based Design"^^xsd:string ;

               rdfs:subClassOf nidm:NIDM_0000155 ;
                  
               obo:IAO_0000115 "A block-based design is a fMRI design comprised of epochs of stimulus and/or behavior."^^xsd:string ;
                  
               obo:IAO_0000116 "Discussed at https://github.com/incf-nidash/nidm/issues/299"^^xsd:string ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#Cartesian

nidm:Cartesian rdf:type owl:Class ;

               rdfs:label "Cartesian"^^xsd:string ;

               obo:IAO_0000115 "Cartesian is a k-space traversal scheme in MRI in which the path of the data acquisition follows a straight line parallel to one axis of the defined k-space and perpendicular to the other k=space axis."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;
               
               rdfs:subClassOf nidm:k-spaceTraversalScheme ;

               owl:synonym nidm:Rectilinear .



###  http://purl.org/nidash/nidm#ClinicalInstrument

nidm:ClinicalInstrument rdf:type owl:Class ;

               rdfs:label "Clinical Instrument"^^xsd:string ;

               obo:IAO_0000115 "A clinical instrument is a instrument usage type that signifies that the type of data to be collected by the instrument is clinically-related data."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;
               
               rdfs:subClassOf nidm:InstrumentUsageType .



###  http://purl.org/nidash/nidm#CoInvestigator

nidm:CoInvestigator rdf:type owl:Class ;
                    
               rdfs:label "Co-Investigator" ;
                    
               rdfs:subClassOf prov:Role ;

               obo:IAO_0000114 obo:IAO_0000428 ;
                    
               obo:IAO_0000115 "A co-investigator is a role for a person that shares responsibility for the oversight and performance of an investigation activity." .



###  http://purl.org/nidash/nidm#CurrentClamp

nidm:CurrentClamp rdf:type owl:Class ;
                  
               rdfs:label "Current Clamp" ;

               rdfs:subClassOf nidm:AcquisitionMethod ;

               obo:IAO_0000115 "A current clamp is an intracellular electrophysiology acquisition method in which current is injected into a cell and the voltage generated by the cell is recorded." ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .                    



###  http://purl.org/nidash/nidm#DemographicsInstrument

nidm:DemographicsInstrument rdf:type owl:Class ;

               rdfs:label "Demographics Instrument"^^xsd:string ;

               obo:IAO_0000115 "A demographics instrument is a instrument usage type that signifies that the type of data to be collected by the instrument is demographics-related data."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;

               rdfs:comment "Common demographics information are variables such as age, handedness, phenotypic sex, ethnicity, and race." ;
               
               rdfs:subClassOf nidm:InstrumentUsageType .




###  http://purl.org/nidash/nidm#DICOMTagCollection

nidm:DICOMTagCollection rdf:type owl:Class ;
                        
               rdfs:label "DICOM Tag Collection"^^xsd:string ;
                        
               rdfs:subClassOf prov:Collection ;
                        
               obo:IAO_0000115 "A DICOM tag collection is a group of identifiers and their values that are stored in the headers of DICOM-formatted acquisition objects and that record the acquisition and reconstruction parameters for that acquisition object."^^xsd:string ;
                        
               obo:IAO_0000114 obo:IAO_0000428 ;
                        
               prov:wasAssociatedWith nidm:AcquisitionObject .




###  http://purl.org/nidash/nidm#DiffusionTensor

nidm:DiffusionTensor rdf:type owl:Class ;
                     
               rdfs:label "Diffusion Tensor"^^xsd:string ;

               obo:IAO_0000115 "Diffusion tensor is an image usage type that denotes that the acquisition object collected can be use to construct the diffusion tensor of the imaged tissue."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ; 
                     
               rdfs:subClassOf nidm:ImageUsageType .



###  http://purl.org/nidash/nidm#DiffusionWeighted

nidm:DiffusionWeighted rdf:type owl:Class ;
                       
               rdfs:label "Diffusion Weighted"^^xsd:string ;

               obo:IAO_0000115 "Diffusion weighted is an image contrast type in which differences in voxel intensity are based on the brownian-motion behavior of the species that generates the detected signal from that voxel."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                       
               rdfs:subClassOf nidm:ImageContrastType .



###  http://purl.org/nidash/nidm#DynamicContrastEnhanced

nidm:DynamicContrastEnhanced rdf:type owl:Class ;
                                
               rdfs:label "Dynamic Contrast Enhanced"^^xsd:string ;

               obo:IAO_0000115 "Dynamic contrast enhanced is a magnetic resonance imaging acquisition method in which T1-weighted images are acquired both before and then sequentially after a contrast agent has been injected in order to quantify metrics related to the permeability of the blood-brain barrier."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;
                                
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#DynamicSusceptibilityContrast

nidm:DynamicSusceptibilityContrast rdf:type owl:Class ;
                                   
               rdfs:label "Dynamic Susceptibility Contrast"^^xsd:string ;

               obo:IAO_0000115 "Dynamic sysceptibility contrast is a magnetic resonance imaging acquisition method in which T2*-weighted images are acquired sequentially after a contrast agent has been injected in order to quantify metrics related to blood volume and flow."^^xsd:string ;

               obo:IAO_0000114 obo:IAO_0000428 ;
                                   
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#EchoPlanar

nidm:EchoPlanar rdf:type owl:Class ;
                
               rdfs:label "Echo Planar"^^xsd:string ;
                
               rdfs:subClassOf nidm:k-spaceTraversalScheme ;
                
               obo:IAO_0000115 "Echo planar is a k-space traversal scheme  used to acquire the k-space data in magnetic resonance imaging in which the entirety of k-space is continuously traversed for each repetition of the pulse sequence."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#Electrocorticography

nidm:Electrocorticography rdf:type owl:Class ;

               obo:IAO_0000115 "Electrocorticography (ECoG) is an electrophysiological acquisition method in which the electrical activity of the brain is measured using electrodes that are placed directly on the cortical surface of the brain."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                          
               rdfs:label "Electrocorticography"^^xsd:string ;
                          
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#Electroencephalography

nidm:Electroencephalography rdf:type owl:Class ;

               obo:IAO_0000115 "Electroencephalography (EEG) is an electrophysiological acquisition method in which the electrical activity of the brain is measured using electrodes that are placed on the surface of the head."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                            
               rdfs:label "Electroencephalography"^^xsd:string ;
                            
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#ElectrophysiologyRecording

nidm:ElectrophysiologyRecording rdf:type owl:Class ;
                                             
               rdfs:label "Electrophysiology Recording"^^xsd:string ;

               obo:IAO_0000115 "Electrophysiology Recording is an acquisition modality in which electrical signals are recorded from one or more electrodes inserted into living tissue."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                                             
               rdfs:subClassOf nidm:AcquisitionModality .



###  http://purl.org/nidash/nidm#NIDM_0000153

nidm:NIDM_0000153 rdf:type owl:Class ;
                  
               rdfs:label "Event-Related Design"^^xsd:string ;
                  
               obo:IAO_0000115 "An event-related design is a fMRI design comprised of discrete events of stimulus and/or behavior."^^xsd:string ;

               rdfs:subClassOf nidm:NIDM_0000155 ;
                  
               obo:IAO_0000116 "Discussed at https://github.com/incf-nidash/nidm/issues/299"^^xsd:string ;
                  
               obo:IAO_0000114 obo:IAO_0000428 .




###  http://purl.org/nidash/nidm#ExtracellularElectrophysiologyRecording

nidm:ExtracellularElectrophysiologyRecording rdf:type owl:Class ;
                                             
               rdfs:label "Extracellular Electrophysiology Recording" ;

               obo:IAO_0000115 "Extracellular Electrophysiology Recording is an acquisition modality in which one or more electrodes are inserted into living tissue and the electrical signals from adjacent cells are recorded."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                                             
               rdfs:subClassOf nidm:ElectrophysiologyRecording .



###  http://purl.org/nidash/nidm#FieldPotential

nidm:FieldPotential rdf:type owl:Class ;
                    
               rdfs:label "Field Potential" ;

               obo:IAO_0000115 "The field potential is an extracellular electrophysiology acquisition method in which an electrode is used to measure the voltage that is generated by synaptic activity (in neural tissue)."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                    
               rdfs:subClassOf nidm:AcquisitionMethod .



###  http://purl.org/nidash/nidm#FlowWeighted

nidm:FlowWeighted rdf:type owl:Class ;
                  
               rdfs:label "Flow Weighted"^^xsd:string ;

               obo:IAO_0000115 "Flow weighted is an image contrast type in which differences in voxel intensity are based on the bulk motion behavior of the species that generates the detected signal from that voxel."^^xsd:string ;
                
               obo:IAO_0000114 obo:IAO_0000428 ;
                  
               rdfs:subClassOf nidm:ImageContrastType .



###  http://purl.org/nidash/nidm#Functional

nidm:Functional rdf:type owl:Class ;

               rdfs:label "Functional"^^xsd:string ;

               obo:IAO_0000115 "Functional is a description of the usage of a given acquisition object, indicating that the purpose of the object is to provide information on the biological function of some aspect of the object on which the acquisition was performed."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                
               rdfs:subClassOf nidm:ImageUsageType .



###  http://purl.org/nidash/nidm#Group

nidm:Group rdf:type owl:Class ;

               rdfs:label "Group"^^xsd:string ;

               obo:IAO_0000115 "A group is a collection of subjects or objects formed by the assignment of membership through the selection of one or more common characteristics."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
           
               rdfs:subClassOf prov:Collection .



###  http://purl.org/nidash/nidm#Gender

nidm:Gender rdf:type owl:Class ;

               rdfs:label "Gender"^^xsd:string ;

               obo:IAO_0000115 "Gender is a social entity that identifies which of either male or female that a person identifies as, which may be different from biological sex."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
           
               rdfs:subClassOf sio:SIO_000009 .



###  http://purl.org/nidash/nidm#ImageContrastType

nidm:ImageContrastType rdf:type owl:Class ;
                       
               rdfs:label "Image Contrast Type"^^xsd:string ;

               obo:IAO_0000115 "Image Contrast Type is an acquisition object quality that denotes the physical properties on which differences in image intensity, in an image, are based."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                       
               rdfs:subClassOf nidm:AcquisitionObjectQuality .



###  http://purl.org/nidash/nidm#ImageDataReconstruction

nidm:ImageDataReconstruction rdf:type owl:Class ;
                             
               rdfs:label "Image Data Reconstruction" ;

               obo:IAO_0000115 "Image data reconstruction is an activity in which raw data is transformed into a matrix of values that represent the spatial distribution of intensity based on some image contrast type."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                             
               rdfs:subClassOf prov:Activity .



###  http://purl.org/nidash/nidm#ImageUsageType

nidm:ImageUsageType rdf:type owl:Class ;
                    
               rdfs:label "Image Usage Type"^^xsd:string ;

               obo:IAO_0000115 "Image Usage Type is an acquisition object quality that denotes to what purpose the acquired object will be employed."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;

               rdfs:comment "Common image usage types in magnetic resonance imaging are anatomical and functional"^^xsd:string ;
                    
               rdfs:subClassOf nidm:AcquisitionObjectQuality .



##  http://purl.org/nidash/nidm#InformedConsentInstrument

nidm:InformedConsentInstrument rdf:type owl:Class ;

               rdfs:label "Informed Consent Instrument"^^xsd:string ;

               obo:IAO_0000115 "An informed consent instrument is a instrument usage type that provides the subject with information regarding the study during which data will be collected from the subject and is used to collect the subjects consent to being a part of that study."^^xsd:string ;
                             
               obo:IAO_0000114 obo:IAO_0000428 ;
               
               rdfs:subClassOf nidm:InstrumentUsageType .



###  http://purl.org/nidash/nidm#InstrumentAdministrator

nidm:InstrumentAdministrator rdf:type owl:Class ;
                                  
               rdfs:label "Instrument Administrator" ;

               obo:IAO_0000115 "An instrument administrator is the role of an agent who/that either provides the instrument or collects the responses of the subject to the contents of the instrument."^^xsd:string ;

               rdfs:comment "The instrument administrator could either be a computer-based system or a person, hence the use of who/that. Common catagories of instruments are behavioral, clinical, or demographic instruments." ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;
                                  
               rdfs:subClassOf prov:Role .



###  http://purl.org/nidash/nidm#InstrumentUsageType

nidm:InstrumentUsageType rdf:type owl:Class ;
                    
               rdfs:label "Instrument Usage Type"^^xsd:string ;

               obo:IAO_0000115 "Instrument Usage Type is an acquisition object quality that denotes to what purpose the acquired object will be employed."^^xsd:string ;
                       
               obo:IAO_0000114 obo:IAO_0000428 ;

               rdfs:comment "Common instrument usage types are behavioral, clinical, and demographics, i.e., the instrument is used to record those types of data."^^xsd:string ;
                    
               rdfs:subClassOf nidm:AcquisitionObjectQuality .



###  http://purl.org/nidash/nidm#Inside-outSpiral

nidm:Inside-outSpiral rdf:type owl:Class ;
                      
               rdfs:label "Inside-out Spiral"^^xsd:string ;
                      
               rdfs:subClassOf nidm:k-spaceTraversalScheme ;
                      
               obo:IAO_0000115 " Inside-out spiral is a k-space traversal scheme in which the path along which magnetic resonance imaging k-space data is acquired is in the shape of a spiral and the data at the center of k-space is acquired first."^^xsd:string ;
                      
               obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#IntracellularElectrophysiologyRecording

nidm:IntracellularElectrophysiologyRecording rdf:type owl:Class ;
                                             
              rdfs:label "Intracellular Electrophysiology Recording" ;
                                             
              rdfs:subClassOf nidm:ElectrophysiologyRecording ;

              obo:IAO_0000115 "Intracellular Electrophysiology Recording is an activity in which electrical measurements of the interior cell environment are made."^^xsd:string ;
                      
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#MagneticResonanceImaging

nidm:MagneticResonanceImaging rdf:type owl:Class ;
                              
              rdfs:label "Magnetic Resonance Imaging" ;
                              
              rdfs:subClassOf nidm:AcquisitionModality ;

              obo:IAO_0000115 "Magnetic Resonance Imaging is an acquisition modality in which radiofrequency electromagnetic radiation is used to excite nuclei of the sample or subject and the characteristics of the detected signal are used to generate intensity contrast between different spatial locations."^^xsd:string ;
                      
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#Magnetoencephalography

nidm:Magnetoencephalography rdf:type owl:Class ;
                            
              rdfs:label "Magnetoencephalography" ;
                            
              rdfs:subClassOf nidm:AcquisitionModality ;

              obo:IAO_0000115 "Magnetoencephalography is an acquisition modality in which the magnetic fields, resulting from electrical activity in the brain, are detected and used to elucidate the time course of brain electrical activity."^^xsd:string ;
                      
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#NIDM_0000154

nidm:NIDM_0000154 rdf:type owl:Class ; 

              rdfs:subClassOf nidm:NIDM_0000155 ;
                  
              rdfs:label "Mixed Design"^^xsd:string ;
                  
              obo:IAO_0000115 "A mixed design is a fMRI design comprised of both epochs & discrete events of stimulus and/or behavior."^^xsd:string ;
                  
              obo:IAO_0000116 "Discussed at https://github.com/incf-nidash/nidm/issues/299"^^xsd:string ;
                  
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#ModelDesigner

nidm:ModelDesigner rdf:type owl:Class ;
                   
              rdfs:label "Model Designer" ;
                   
              rdfs:subClassOf prov:Role ;
                   
              obo:IAO_0000115 "A model designer is a role for an agent who creates the model that describes an experiment" ;

              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#ModelSpecification

nidm:ModelSpecification rdf:type owl:Class ;
                        
              rdfs:label "Model Specification" ;
                        
              rdfs:subClassOf prov:Activity ;

              obo:IAO_0000115 "Model specification is an activity in which the model designer creates the model that describes an experiment" ;

              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#MultiUnitReccording

nidm:MultiUnitReccording rdf:type owl:Class ;
                         
              rdfs:label "Multi-unit Recording" ;
                         
              rdfs:subClassOf nidm:AcquisitionMethod ;

              obo:IAO_0000115 "Multiunit recording is an extracellular electrophysiological acquisition method in which an acquisition object is gathered using multiple recording devices." ;

              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#NIDM_0000155

nidm:NIDM_0000155 rdf:type owl:Class ;
                  
              rdfs:label "fMRI Design Type"^^xsd:string ;
                  
              rdfs:subClassOf nidm:AcquisitionObjectQuality ;
                  
              obo:IAO_0000116 "Discussed at https://github.com/incf-nidash/nidm/issues/299"^^xsd:string ;
                  
              obo:IAO_0000115 "An fMRI design type is the type of stimulus presentation used in the data acquisition; one of block-based design, event-related design or mixed design."^^xsd:string ;
                  
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#NuclearMagneticResonanceSpectroscopy

nidm:NuclearMagneticResonanceSpectroscopy rdf:type owl:Class ;
                                          
              rdfs:label "Nuclear Magnetic Resonance Spectroscopy" ;
                                          
              rdfs:subClassOf nidm:AcquisitionModality ;

              obo:IAO_0000115 "Nuclear magnetic resonance spectroscopy is an acquisition modality in which radiofrequency electromagnetic radiation is used to excite nuclei of the sample or subject, the characteristics of the detected signal are used to generate signal intensity contrast over an entire selected volume, and the resulting signal is displayed as a signal spectrum rather than as matrix meant to be a representation of the underlying object."^^xsd:string ;

              rdfs:comment "The volume of interest may be selected either by the natural extent of the excited volume (e.g., surface coils) or by selecting for excitation, a thick slab through the use of magnetic field gradients."^^xsd:string ;
                      
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#NuclearMagneticResonanceSpectroscopicImaging

nidm:NuclearMagneticResonanceSpectroscopicImaging rdf:type owl:Class ;
                                          
              rdfs:label "Nuclear Magnetic Resonance Spectroscopic Imaging" ;
                                          
              rdfs:subClassOf nidm:AcquisitionModality ;

              obo:IAO_0000115 "Nuclear magnetic resonance spectroscopic imaging is an acquisition modality in which radiofrequency electromagnetic radiation is used to excite nuclei of the sample or subject, the characteristics of the detected signal are used to generate intensity contrast over a restricted (usually grid-like) area, and the resulting signal is displayed as a spectrum for each one of the spatially-resolved areas."^^xsd:string ;
                      
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#Outside-inSpiral

nidm:Outside-inSpiral rdf:type owl:Class ;
                      
              rdfs:label "Outside-in Spiral"^^xsd:string ;
                      
              rdfs:subClassOf nidm:k-spaceTraversalScheme ;
                      
              obo:IAO_0000115 "Outside-inSpiral is a k-space traversal scheme in which the path, along which magnetic resonance imaging k-space data is acquired, is spiral and the data further away from the center of k-space is acquired first."^^xsd:string ;
                      
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#ParallelImaging

nidm:ParallelImaging rdf:type owl:Class ;
                           
              rdfs:label "Parallel Imaging"^^xsd:string ;
                           
              rdfs:subClassOf nidm:AcquisitionMethod ;
                           
              obo:IAO_0000115 "Parallel imaging is an acquisition method used in magnetic resonance imaging to acquire the k-space data in which multiple coils are employed and the signals from each coil are disambiguated using the coil sensitivity profile."^^xsd:string ;
                           
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#PatchClamp

nidm:PatchClamp rdf:type owl:Class ;
                
              rdfs:label "Patch Clamp" ;
                
              rdfs:subClassOf nidm:AcquisitionMethod ;

              obo:IAO_0000115 "Patch clamp is an intracellular electrophysiological acquisiiton method in which a micropipette tip is brought near a cell and scution applied drawing a section of cell membrane into the tip in order that ion movement or the intracellular environment can be studied."^^xsd:string ;
                           
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#PerformedPlan

nidm:PerformedPlan rdf:type owl:Class ;
                   
              rdfs:label "Performed Plan"^^xsd:string ;
                   
              rdfs:subClassOf prov:Plan ;
                   
              obo:IAO_0000115 "A performed plan is a plan that is actually implemented during an acquisition activity."^^xsd:string ;
                   
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#PositronEmissionTomography

nidm:PositronEmissionTomography rdf:type owl:Class ;
                                
              rdfs:label "Positron Emission Tomography" ;
                                
              rdfs:subClassOf nidm:AcquisitionModality ;

              obo:IAO_0000115 "Positron emission tomography is an acquisition modality in which a positron-emitting radioactive tracer is introduced into the subject and the gamma rays emitted after a positron-electron collision are detected and their originating position calculated."^^xsd:string ;
                   
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#PresentationSoftware

nidm:PresentationSoftware rdf:type owl:Class ;
                          
              rdfs:subClassOf prov:SoftwareAgent ;

              rdfs:label "Presentation Software"^^xsd:string ;

              obo:IAO_0000115 "Presentation software is a software agent that provides the stimulus to the subject during a functional imaging (MRI, MEG, EEG) or behavioral data acquisition activity."^^xsd:string ;
                   
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#PrincipalInvestigator

nidm:PrincipalInvestigator rdf:type owl:Class ;
                           
              rdfs:label "Principal Investigator"^^xsd:string ;
                           
              rdfs:subClassOf prov:Role ;
                           
              obo:IAO_0000115 "A principal investigator is a role for a person who leads an activity during which data is acquired with a specific scientific purpose."^^xsd:string ;
                           
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#ProcessedAcquisitionObject

nidm:ProcessedAcquisitionObject rdf:type owl:Class ;
                                    
              rdfs:label "Processed Acquisition Object"^^xsd:string ;
                                    
              rdfs:subClassOf nidm:AcquisitionObject ;

              obo:IAO_0000115 "A processeded acquisition object is an acquisition object that has undergone an image reconstruction activity resulting in a matrix of values that when viewed form an image."^^xsd:string ;

              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#Project

nidm:Project rdf:type owl:Class ;
             
              rdfs:label "Project"^^xsd:string ;
             
              rdfs:subClassOf prov:Activity ;
             
              obo:IAO_0000115 "A project is a coordinated set of activities designed to generate data acquisition objects, with the ultimate goal of discovery or hypothesis testing."^^xsd:string ;
             
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#Protocol

nidm:Protocol rdf:type owl:Class ;
              
              rdfs:label "Protocol"^^xsd:string ;
              
              rdfs:subClassOf nidm:SpecifiedPlan ;
              
              obo:IAO_0000115 "An protocol is a specified plan that contains information necessary to acquire the desired data during a session and the acquisition device parameters that are to be used in that acquisition."^^xsd:string ;

              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#ProtonDensityWeighted

nidm:ProtonDensityWeighted rdf:type owl:Class ;
                           
              rdfs:label "Proton Density Weighted" ;
                           
              rdfs:subClassOf nidm:ImageContrastType ;

              obo:IAO_0000115 "Proton density weighted is a image contrast type in magnetic resonance imaging in which differences in voxel intensity reflect underlying differences in the number of protons in those voxels."^^xsd:string ;

              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#PulseSequence

nidm:PulseSequence rdf:type owl:Class ;
                   
              rdfs:label "Pulse Sequence"^^xsd:string ;
                   
              rdfs:subClassOf prov:Collection ;
                   
              obo:IAO_0000115 "A pulse sequence is a collection of commands sent to the magnetic resonance imaging or nuclear magnetic resonance hardware to control the excitation of the subject or sample and the acquisition of data."^^xsd:string ;
                   
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#RawAcquisitionObject

nidm:RawAcquisitionObject rdf:type owl:Class ;
                          
              rdfs:label "Raw Acquisition Object"^^xsd:string ;
                          
              rdfs:subClassOf nidm:AcquisitionObject ;
                          
              obo:IAO_0000115 "A raw acquisition object is an acquisition object that has not yet undergone any further processing steps such asthe image reconstruction process."^^xsd:string ;

              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#ReconstructedAcquisitionObject

nidm:ReconstructedAcquisitionObject rdf:type owl:Class ;
                                    
              rdfs:label "Reconstructed Acquisition Object"^^xsd:string ;
                                    
              rdfs:subClassOf nidm:AcquisitionObject ;

              obo:IAO_0000115 "A reconstructed acquisition object is an acquisition object that has undergone an image reconstruction activity resulting in a matrix of values that when viewed form an image."^^xsd:string ;

              obo:IAO_0000114 obo:IAO_0000428 .




###  http://purl.org/nidash/nidm#Rectilinear

nidm:Rectilinear rdf:type owl:Class ;

              rdfs:label "Rectilinear"^^xsd:string ;

              obo:IAO_0000115 "Rectilinear is a k-space traversal scheme in magnetic resonance imaging in which the path in k-space over which the data is acquired is linear and aligned perpendicular to either the phase-encoding or frequency-encoding direction."^^xsd:string ;

              owl:synonym "Cartesian"^^xsd:string ;
                 
              rdfs:subClassOf nidm:k-spaceTraversalScheme ;

              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#ResearchAssistant

nidm:ResearchAssistant rdf:type owl:Class ;
                       
              rdfs:label "Research Assistant"^^xsd:string ;
                       
              rdfs:subClassOf prov:Role ;
                       
              obo:IAO_0000114 "A research assistant is a role for a person who participates in laboratory- and study-related activities, but who is not leading the project in any fashion."^^xsd:string ;
                       
              obo:IAO_0000115 obo:IAO_0000428 .
                       


###  http://purl.org/nidash/nidm#Series

nidm:Series rdf:type owl:Class ;

              rdfs:subClassOf nidm:Acquisition ;
            
              rdfs:label "Series"^^xsd:string ;

              provone:isPartOf nidm:Session ;
            
              obo:IAO_0000115 "A series is an activity in which data of a single type is acquired such that one acquisition takes place during the same temporal epoch."^^xsd:string ;
            
              rdfs:comment "During a MRI session, for example, T1-weighted and resting-state data were acquired.  Both the T1-weighted and resting state data are Acquisition Objects and the activity during which they were acquired is the Session. The Series is the activity that led to the production of each Acquisition Object." ;

              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#Session

nidm:Session rdf:type owl:Class ;
             
              rdfs:subClassOf prov:Activity ;

              rdfs:label "Session"^^xsd:string ;
             
              obo:IAO_0000115 "A session is an activity during which data, potentially of different types, is acquired from a subject by an experimenter during a single unbroken time period." ;

              rdfs:comment "During a MRI session, for example, T1-weighted and resting-state data were acquired.  Both the T1-weighted and resting state data are Acquisition Objects and the time period during which they were acquired is the Session." ;
             
              provone:isPartOf nidm:Project ;

              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#SessionObject

nidm:SessionObject rdf:type owl:Class ;
                   
              rdfs:label "Session Object"^^xsd:string ;
                   
              rdfs:subClassOf prov:Collection ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty dct:hasPart ;
                                owl:someValuesFrom nidm:AcquisitionObject
                              ] ;
                   
              obo:IAO_0000115 "A session object is an entity that represents one or more acquisitions that take place during the same temporal epoch (session)."^^xsd:string ;
                   
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#SharpElectrode

nidm:SharpElectrode rdf:type owl:Class ;
                    
              rdfs:label "Sharp Electrode" ;
                    
              rdfs:subClassOf nidm:AcquisitionMethod ;

              obo:IAO_0000115 "Sharp electrode is intracellular electrophysiological acquisition method used for measuring the potential of the intracellular environment with minimal disturbance of that environment, in which a pipette, filled with electrolyte and with a very small opening, is inserted into the cell and the potential measured."^^xsd:string ;
                   
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#SimultaneousMultisliceMethod

nidm:SimultaneousMultisliceMethod rdf:type owl:Class ;
                                  
              rdfs:label "Simultaneous Multislice Method"^^xsd:string ;
                                  
              rdfs:subClassOf nidm:AcquisitionMethod ;
                                  
              obo:IAO_0000115 "Simultaneous multislice is an acquisition method in magnetic resonance imaging used to acquire the k-space in which data from multiple slices are acquired at the same time and the coil sensitivity profiles are used to separate the data from each slice."^^xsd:string ;
                                  
              obo:IAO_0000114 obo:IAO_0000125 .



###  http://purl.org/nidash/nidm#SingleUnitReccording

nidm:SingleUnitReccording rdf:type owl:Class ;
                          
              rdfs:label "Single-unit Recording" ;
                          
              rdfs:subClassOf nidm:AcquisitionMethod ;

              obo:IAO_0000115 "Single-unit recording is an extracellular electrophysiological acquisition method, in which a thin electrode is introduced into the extracellular space of a sample or subject and the action potential from a single neuron is recorded."^^xsd:string ;
                                  
              obo:IAO_0000114 obo:IAO_0000125 .



###  http://purl.org/nidash/nidm#SpecifiedPlan

nidm:SpecifiedPlan rdf:type owl:Class ;
                   
              rdfs:label "Specified Plan"^^xsd:string ;
                   
              rdfs:subClassOf prov:Plan ;
                   
              obo:IAO_0000115 "A specified plan is a plan that describes how the acquisition activity is to be carried out."^^xsd:string ;

              rdfs:comment "Note that this is related to nidm:PerformedPlan, which is the plan that is actually carried out." ;
                   
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#QuantitativeSusceptibilityMapping

nidm:QuantitativeSusceptibilityMapping rdf:type owl:Class ;
                                   
              rdfs:label "Quantitative Susceptibility Mapping"^^xsd:string ;
                                   
              rdfs:subClassOf nidm:ImageUsageType ;

              obo:IAO_0000115 "Quantitative susceptibility mapping is a magentic resonance imaging image-usage type in which differences in voxel intensity reflect quantitative differences magnetic susceptibilty of the underlying sample."^^xsd:string ;
             
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#StimulusPresentationFile

nidm:StimulusPresentationFile rdf:type owl:Class ;
                              
              rdfs:label "Stimulus Presentation File"^^xsd:string ;
                              
              rdfs:subClassOf nidm:AuxiliaryFile ;
                              
              obo:IAO_0000115 "A stimulus presentation file is an auxiliary files that contains the presentation timing and/or location of the experimental stimulus (e.g., words, pictures) that are shown to the subject in a task-based assessment activity."^^xsd:string ;
                                
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#StimulusResponseFile

nidm:StimulusResponseFile rdf:type owl:Class ;
                          
              rdfs:label "Stimulus Response File"^^xsd:string ;
                          
              rdfs:subClassOf nidm:AuxiliaryFile ;
                          
              obo:IAO_0000115 "A stimulus response file is an auxiliary file that contains the recorded response of the subject to an experimental stimulus the course of a task-based assessment activity."^^xsd:string ;
                          
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#Structural

nidm:Structural rdf:type owl:Class ;
                
              rdfs:subClassOf nidm:ImageUsageType ;

              rdfs:label "Structural"^^xsd:string ;
                          
              obo:IAO_0000115 "A stimulus response file is an auxiliary file that contains the recorded response of the subject to an experimental stimulus the course of a task-based assessment activity."^^xsd:string ;
                          
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#Subject

nidm:Subject rdf:type owl:Class ;
             
              rdfs:label "Subject"^^xsd:string ;
             
              rdfs:subClassOf prov:Role ;
             
              obo:IAO_0000115 "A subject is a role for an agent (either living or non-living) which is characterized by being the object of study in an acquisition activity."^^xsd:string ;
             
              obo:IAO_0000114 obo:IAO_0000428 .




###  http://purl.org/nidash/nidm#SusceptibilityWeighted

nidm:SusceptibilityWeighted rdf:type owl:Class ;
                            
              rdfs:label "Susceptibility Weighted"^^xsd:string ;
                            
              rdfs:subClassOf nidm:ImageContrastType ;

              obo:IAO_0000115 "Susceptibility weighted is a magentic resonance imaging image-contrast type in which differences in voxel intensity reflect differences in magnetic susceptibilty of the underlying sample."^^xsd:string ;
             
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#SusceptibilityWeightedImaging

nidm:SusceptibilityWeightedImaging rdf:type owl:Class ;
                                   
              rdfs:label "Susceptibility Weighted Imaging"^^xsd:string ;
                                   
              rdfs:subClassOf nidm:ImageUsageType ;

              obo:IAO_0000115 "Susceptibility weighted imaging is a magentic resonance imaging image-usage type in which differences in voxel intensity reflect differences in magnetic susceptibilty of the underlying sample, but are not caculated values."^^xsd:string ;
             
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#T1Weighted

nidm:T1Weighted rdf:type owl:Class ;
                
              rdfs:label "T1 Weighted"^^xsd:string ;
                
              rdfs:subClassOf nidm:ImageContrastType ;

              obo:IAO_0000115 "T1 weighted is an image contrast type in which differences in voxel intensity reflect differences in the spin-lattice relaxation time of the probe in the underlying sample."^^xsd:string ;
             
              obo:IAO_0000114 obo:IAO_0000428 .



###  http://purl.org/nidash/nidm#T2StarWeighted

